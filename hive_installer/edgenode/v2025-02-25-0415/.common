#
# Common shell functions and defaults
#

# Check if SystemD is installed.
hassystemd=false
if [ "`cat /proc/1/comm 2>&1`" = "systemd" ]; then
    hassystemd=true
fi

APP_NAME="PaperCut Print Provider"

APP_NAME_SHORT=papercut

HOST_USER=papercut

currentdir="$(dirname "$(test -L "$0" && readlink "$0" || echo "$0")")"

SCRIPT_HOME="`cd \"${currentdir}\"; pwd`"

PRINT_PROVIDER_HOME="`cd \"${SCRIPT_HOME}/../\"; pwd`"

CUPS_PREFIX=papercut

#Flag file used to mark ios printing as not starting
IOSPRINT_DISABLE_FLAG="${SCRIPT_HOME}/IOSPRINT_DISABLE_FLAG"
